{
    "description": "Shows the GoldBet fastcode for any match on diretta.it",
    "manifest_version": 2,
    "name": "DirettaGold",
    "version": "1.0",
    "icons": {
        "48": "icons/direttagold-48.png",
        "96": "icons/direttagold-96.png"
    },
    "permissions": [
        "https://www.goldbet.it/scommesse/searchEvents/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://www.diretta.it/partita/*"
            ],
            "js": [
                "direttagold.js"
            ]
        }
    ]
}